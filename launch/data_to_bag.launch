<launch>
	<node pkg="data_collect" type="data_to_bag" name="data_to_bag" output="screen" clear_params="true">
		<param name="dataset_path" type="string" value="$(find data_collect)/output/" />
		<rosparam param="directory_names">["image","imu","fix","odometry_filtered","zed_odom","velodyne","speed","steer","radar"]</rosparam>
		<rosparam param="topic_names">["/zed/zed_node/right_raw/image_raw_color","/imu/data","/fix","/odometry/filtered","/zed/zed_node/odom","/velodyne_points","/speed_can","/steer_can","/radar/target_list_cartesian"]</rosparam>
		<rosparam param="frame_ids">["zed_right_camera_optical_frame","imu","gps","base_link","base_link","velodyne","base_link","base_link","radar"]</rosparam>
		<rosparam param="data_types">["Image","Imu","NavSatFix","Odometry","Odometry","PointCloud2","Float32Stamped","Float32Stamped","PointCloud2"]</rosparam>
	</node>
</launch>