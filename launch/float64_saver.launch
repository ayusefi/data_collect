<launch>
	<node pkg="data_collect" type="float64_saver" name="float64_saver_steer_node" output="screen" clear_params="true">
		<param name="float64_topic" type="string" value="/steer_can" />
		<param name="output_path" type="string" value="$(find data_collect)/output/steer/" />
	</node>
	
	<node pkg="data_collect" type="float64_saver" name="float64_saver_speed_node" output="screen" clear_params="true">
		<param name="float64_topic" type="string" value="/speed_can" />
		<param name="output_path" type="string" value="$(find data_collect)/output/speed/" />
	</node>
</launch>

